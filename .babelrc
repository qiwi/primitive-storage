{
  "plugins": [
    "@babel/plugin-proposal-export-default-from",
    "@babel/plugin-proposal-object-rest-spread",
    "transform-class-properties"
  ],
  "comments": false,
  "ignore": [
    "interface.js"
  ],

  "env": {
    "test": {
      "presets": [
        "@babel/preset-flow",
        "@babel/preset-env"
      ],
      "plugins": [
        "@babel/plugin-proposal-export-default-from",
        "@babel/plugin-proposal-object-rest-spread",
        "transform-class-properties"
      ]
    },
    "production": {
      "presets": [
        ["@babel/preset-env", {
          "targets": {
            "browsers": ["last 2 versions"]
          }
        }],
        "@babel/preset-flow",
        "minify"
      ],
      "plugins": [
        "@babel/plugin-proposal-export-default-from",
        "@babel/plugin-proposal-object-rest-spread",
        "transform-class-properties"
      ]
    }
  }
}

